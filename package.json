    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "bulma": "^0.7.5",
    "history": "^4.9.0",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "nexmo": "^2.4.1",
    "random": "^2.1.1",
    "react": "^16.8.6",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-webcam": "^1.1.1",
    "simpl-schema": "^1.10.2"
  },
  "meteor": {
